#include <bits/stdc++.h>
using namespace std;
#define speed ios_base::sync_with_stdio(NULL);cin.tie(NULL);cout.tie(NULL);
#define ll long long
#define FOR(a,b,i) for(int i=a;i<=b;i++)
int n,x,k;
int a[1005];
int res(int x,int i)
{
  int d=0,s=0;
  while(x>0)
  {
    d++;
    s=s+(x%10);
    x/=10;
  }
  if(d==1) x=a[i-1]*2;
  else x=s;
  return x;
}
int main() 
{
  speed
int du[11];
du[0]=14,du[1]=5,du[2]=10,du[3]=1,du[4]=2,du[5]=4,du[6]=8,du[7]=16,du[8]=7;
